<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>register Form</display-name>

	<context-param>
		<param-name>Sport</param-name>
		<param-value>Cricket</param-value>
	</context-param>


	<!-- Servlet Decelaration -->
	<servlet>
		<servlet-name>Servlet</servlet-name>
		<servlet-class>Servlet_Sample.ServletExample</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>GenericServlet</servlet-name>
		<servlet-class>Servlet_Sample.GenericServletExample</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>HttpServlet</servlet-name>
		<servlet-class>Servlet_Sample.HttpServletExample</servlet-class>
	</servlet>
	<!-- Register Form -->
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>First_Project.RegisterForm</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>success</servlet-name>
		<servlet-class>First_Project.Success_Servlet</servlet-class>
	</servlet>

	<!-- Send Redirect -->
	<servlet>
		<servlet-name>sendredirect</servlet-name>
		<servlet-class>Request_Collabration.SendRedirect</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>sendredirectdispacther</servlet-name>
		<servlet-class>Request_Collabration.SendRedirectDispacther1</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>sendredirectdispacther2</servlet-name>
		<servlet-class>Request_Collabration.SendRedirectDispacther2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>requestdispacther</servlet-name>
		<servlet-class>Request_Collabration.Request_Dispacther</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>requestdispacther2</servlet-name>
		<servlet-class>
			Request_Collabration.Request_Dispacther2</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>servletcontextconfig</servlet-name>
		<servlet-class>
			ServletContext_And_ServletConfig.Servlet1</servlet-class>
		<init-param>
			<param-name>Name</param-name>
			<param-value>Ramakrishna</param-value>
		</init-param>
		<init-param>
			<param-name>Email</param-name>
			<param-value>ramakrishnamagadum@gmail.com</param-value>
		</init-param>
	</servlet>


	<servlet>
		<servlet-name>servletcontextconfig2</servlet-name>
		<servlet-class>
			ServletContext_And_ServletConfig.Servlet2</servlet-class>
	</servlet>




	<!-- Mapping -->
	<servlet-mapping>
		<servlet-name>Servlet</servlet-name>
		<url-pattern>/servlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>GenericServlet</servlet-name>
		<url-pattern>/genericServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>HttpServlet</servlet-name>
		<url-pattern>/httpServlet</url-pattern>
	</servlet-mapping>

	<!-- Register Form -->
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/Registerform</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>success</servlet-name>
		<url-pattern>/Success</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>sendredirect</servlet-name>
		<url-pattern>/SendRedirect</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>sendredirectdispacther</servlet-name>
		<url-pattern>/SendRedirectDispacther</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>sendredirectdispacther2</servlet-name>
		<url-pattern>/SendRedirectDispacther2</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>requestdispacther</servlet-name>
		<url-pattern>/RequestDispacther</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>requestdispacther2</servlet-name>
		<url-pattern>/RequestDispacther2</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>servletcontextconfig</servlet-name>
		<url-pattern>/Servlet1</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>servletcontextconfig2</servlet-name>
		<url-pattern>/Servlet2</url-pattern>
	</servlet-mapping>

	<!-- Welocome file tag -->
	<welcome-file-list>
		<welcome-file>home.html</welcome-file>
	</welcome-file-list>
</web-app>
